$button201013Activate.Add_Click({


    ShowDialogInProgress
    Set-Location "$Env:SystemRoot\system32"
    if((Get-Service LicenseManager).Status -ne "Running"){ Start-Service LicenseManager }
    if (Test-Path "$Env:ProgramFiles\Microsoft Office\Office15\ospp.vbs" -PathType leaf){Set-Location "$Env:ProgramFiles\Microsoft Office\Office15"}
    if (Test-Path "$Env:ProgramFiles (x86)\Microsoft Office\Office15\ospp.vbs" -PathType leaf){Set-Location "$Env:ProgramFiles (x86)\Microsoft Office\Office15"}
    if (Test-Path "$Env:ProgramFiles\Microsoft Office\Office14\ospp.vbs" -PathType leaf){Set-Location "$Env:ProgramFiles\Microsoft Office\Office14"}
    if (Test-Path "$Env:ProgramFiles (x86)\Microsoft Office\Office14\ospp.vbs" -PathType leaf){Set-Location "$Env:ProgramFiles (x86)\Microsoft Office\Office14"}
    Write-Host @"

>> Supported products:
>> Microsoft Office Standard 2010
>> Microsoft Office Professional Plus 2010
>> Microsoft Office Standard 2013
>> Microsoft Office Professional Plus 2013

"@

    Write-Host ">> Initializing..."


    (New-Object System.Net.WebClient).DownloadFile("https://get.msguides.com/office2010/Office_2010_Library.zip", "$env:TEMP\Office_2010_Library.zip")
    (New-Object System.Net.WebClient).DownloadFile("https://get.msguides.com/office2013/Office_2013_Library.zip", "$env:TEMP\Office_2013_Library.zip")

    Expand-Archive "$env:TEMP\Office_2010_Library.zip" -DestinationPath  "$env:TEMP\" -Force 
    Expand-Archive "$env:TEMP\Office_2013_Library.zip" -DestinationPath  "$env:TEMP\" -Force 
    del "$env:TEMP\Office_2010_Library.zip"
    del "$env:TEMP\Office_2013_Library.zip"


    $pathlib =  "$env:TEMP\library"

    $files = Get-ChildItem -Path $pathlib -name
    foreach($i in 1..10){
        $hiden = cscript //nologo //b ospp.vbs /inslic:$files[$i]
    }
    Remove-Item $pathlib -Recurse -Force

    Write-Host ">> Activating Office..."


    $hiden = ""
    $hiden = cscript //nologo slmgr.vbs /ckms
    $hiden = cscript //nologo ospp.vbs /setprt:1688
    $hiden = cscript //nologo ospp.vbs /unpkey:H3GVB
    $hiden = cscript //nologo ospp.vbs /unpkey:GVGXT

    $hiden1 =cscript //nologo ospp.vbs /inpkey:VYBBJ-TRJPB-QFQRF-QFT4D-H3GVB
    $hiden2 =cscript //nologo ospp.vbs /inpkey:YC7DK-G2NP3-2QQC3-J6H88-GVGXT

    if ($hiden1 -match "ERROR" -and $hiden2 -match "ERROR"){Write-Host "Unable to install Product key. Please recheck that you have selected the correct Office Version from PowerKit.
if you have selected the Correct version then Sorry your Version is not Supported." -ForegroundColor cyan

    }else{  

    Write-Host ">> Please wait..."

    For ()  {

        $return =  cscript //nologo ospp.vbs /sethst:kms8.MSGuides.com
        if ($return -match "successful"){$return =  cscript //nologo ospp.vbs /act ; if($return -match "successful"){write-host "Microsoft Office is activated successfully." -ForegroundColor green ; Break}}

        $return =  cscript //nologo ospp.vbs /sethst:kms9.MSGuides.com
        if ($return -match "successful"){$return =  cscript //nologo ospp.vbs /act ; if($return -match "successful"){write-host "Microsoft Office is activated successfully." -ForegroundColor green ; Break}}


        Write-Host @"

Microsoft Key Management Service didn't able to activate your Office.
- Select the correct version of office from windows PowerKit
- Make sure that office is installed in Default "C" drive
- Make sure that you're connected to the internet
- Make sure that Window Script Host(WSH) is enabled
- Try again later "Microsoft(KMS) server can be busy"
"@ -ForegroundColor cyan
 
        Break
        }
    
    }
    Set-Location "$Env:SystemRoot\system32"
    HideDialogInProgress



})
$button201013Deactivate.Add_Click($deactivateAllOffice)









$buttonhome.Add_Click({ 

    $msgBody = "Windows Powerkit will perform some registry edits to change the windows to Home edition. Then you have to reinstall windows without formatting or erasing your data. 
In windows installation, windows will automatically read the registry and set the installation to Home edition 

Reinstallation is necessary for downgrade because windows will remove some features which are present in other editions.

Visit Microsoft website and download the media creation tool."
    $Result = [System.Windows.MessageBox]::Show($msgBody,"Upgrade Process","YesNo","Information")

    if($Result -eq "Yes"){
    
        if((Get-CimInstance Win32_OperatingSystem).Caption -notmatch "Windows 11|Windows 10"){ Write-Host "`nOnly Windows 10 and 11 are Supported for Edition Downgrade" ; return}
        if((Get-CimInstance Win32_OperatingSystem).Caption -match "Home"){ Write-Host "`nYour windows is already At Home Edition"; return }


        Set-ItemProperty -Path "HKLM:\Software\Microsoft\Windows NT\CurrentVersion" -Name EditionID -Type String -Value "Core"
        Set-ItemProperty -Path "HKLM:\Software\Microsoft\Windows NT\CurrentVersion" -Name CompositionEditionID -Value "Core"
        Set-ItemProperty -Path "HKLM:\Software\Wow6432Node\Microsoft\Windows NT\CurrentVersion" -Name EditionID -Type String -Value "Core"
        Set-ItemProperty -Path "HKLM:\Software\Wow6432Node\Microsoft\Windows NT\CurrentVersion" -Name CompositionEditionID -Value "Core"

        if((Get-CimInstance Win32_OperatingSystem).Caption -match "Windows 10"){

            Set-ItemProperty -Path "HKLM:\Software\Microsoft\Windows NT\CurrentVersion" -Name ProductName -Value "Windows 10 Home"
            Set-ItemProperty -Path "HKLM:\Software\Wow6432Node\Microsoft\Windows NT\CurrentVersion" -Name ProductName -Value "Windows 10 Home"

            Write-Host "Windows Edition is Set to Home in Registry. Download and Run Media Creation tool to downgrade to windows Home." -ForegroundColor Cyan
            Start-Process https://www.microsoft.com/en-us/software-download/windows10

        }elseif((Get-CimInstance Win32_OperatingSystem).Caption -match "Windows 11"){

            Set-ItemProperty -Path "HKLM:\Software\Microsoft\Windows NT\CurrentVersion" -Name ProductName -Value "Windows 11 Home"
            Set-ItemProperty -Path "HKLM:\Software\Wow6432Node\Microsoft\Windows NT\CurrentVersion" -Name ProductName -Value "Windows 11 Home"

            Write-Host "Windows Edition is Set to Home in Registry. Download and Run Media Creation tool to downgrade to windows Home." -ForegroundColor Cyan
            Start-Process https://www.microsoft.com/en-us/software-download/windows11
        }

    }


})
$buttonEducation.Add_Click({ 
    Change-Edition "Education"
})
$buttonProfessional.Add_Click({ 
    Change-Edition "Pro|Professional"
 })
$buttonEnterprise.Add_Click({
    Change-Edition "Enterprise"
})




$btnCreateRestorePoint.Add_Click({  

      ShowDialogInProgress
      Enable-ComputerRestore -Drive "C:\"
      REG ADD "HKLM\SOFTWARE\Microsoft\Windows NT\CurrentVersion\SystemRestore" /V "SystemRestorePointCreationFrequency" /T REG_DWORD /D 0 /F
      Checkpoint-Computer -Description "Revert Optimization" -RestorePointType "MODIFY_SETTINGS"
      Write-Host "`nRestorePoint has been Created" -ForegroundColor green
      HideDialogInProgress

  })
$btnRestore.Add_Click({  
      rstrui.exe
  })



$btnOfficialSite.add_Click({ 
    Start-Process "https://bit.ly/3TO9VaL"
 })


